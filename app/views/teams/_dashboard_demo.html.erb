  <% if (@demo) %>
    <div class="row">
      <div class="large-12 columns">
        <div class="panel bg-lightblue clearfix">
          <h3 class="left no-margin">Example Team</h3>
            <h6 class="left subheader league-type-header margin-left-2">
              <a>Help Me</a>
            </h6>
        </div>
      </div>

      <div class="large-12 columns margin-bottom-1">
        <ul class="panel large-6 columns">
          <h5 class="no-margin"><a href="/teams/showbatters?id=<%=@demo_team._id%>"><%=@demo_team.team_name%></a></h5>
          <h6 class="subheader"><%=@demo_team.league_name%></h6>

          <% if (@demo_team.weekly_team) %>
          <% else %>
            <% if (@demo_team.daily_auto_batter) %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "b", 
                :var_active_hide => "", 
                :var_inactive_hide => "hide", 
                :var_text => "Batters", 
                :var_slug => "showbatters", 
                :var_link_text => "[ Edit ]",
                :var_active_checked => "checked", 
                :var_inactive_checked => ""
              } %>
            <% else %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "b", 
                :var_active_hide => "hide", 
                :var_inactive_hide => "", 
                :var_text => "Batters", 
                :var_slug => "showbatters", 
                :var_link_text => "[ Edit ]",
                :var_active_checked => "", 
                :var_inactive_checked => "checked"
              } %>
            <% end %>

            <% if (@demo_team.daily_auto_pitcher) %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "p", 
                :var_active_hide => "", 
                :var_inactive_hide => "hide", 
                :var_text => "Pitchers", 
                :var_slug => "showpitchers", 
                :var_link_text => "[ Edit ]",
                :var_active_checked => "checked", 
                :var_inactive_checked => ""
              } %>
            <% else %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "p", 
                :var_active_hide => "hide", 
                :var_inactive_hide => "", 
                :var_text => "Pitchers", 
                :var_slug => "showpitcher",
                :var_link_text => "[ Edit ]",
                :var_active_checked => "", 
                :var_inactive_checked => "checked"
              } %>
            <% end %>

            <% if (@demo_team.real_time_batter) %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "r", 
                :var_active_hide => "", 
                :var_inactive_hide => "hide", 
                :var_text => "Real Time", 
                :var_slug => "", 
                :var_link_text => "",
                :var_active_checked => "checked", 
                :var_inactive_checked => ""
              } %>
            <% else %>
              <%= render :partial => "dashboard_team", 
                :object => @demo_team, :as => "team",
                :locals => {:var_type => "r", 
                :var_active_hide => "hide", 
                :var_inactive_hide => "", 
                :var_text => "Real Time", 
                :var_slug => "",
                :var_link_text => "",
                :var_active_checked => "", 
                :var_inactive_checked => "checked"
              } %>
            <% end %>
          <% end %>
        </ul>
        <div class="panel large-6 columns bg-lightblue">
          <p>If you are having issues adding your Yahoo/ESPN accounts, please open a new web browser.  Log out of all Yahoo/ESPN sessions and log back in. Afterwards, try adding your account on RotoStarter again.  If you are still having trouble, please contact us at <a href="mailto:support@rotostarter.com">support@rotostarter.com</a>.</p>
        </div>
      </div>
    </div>
  <% end %>